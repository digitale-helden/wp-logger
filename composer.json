{
  "name": "digitale-helden/wp-logger",
  "description": "Logger with GELF and log file rotation using monolog",
  "type": "library",
  "keywords": [
    "php",
    "logger",
    "monolog",
    "graylog"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "setcooki",
      "email": "set@cooki.me",
      "homepage": "http://set.cooki.me"
    }
  ],
  "support": {
    "issues": "https://github.com/digitale-helden/wp-logger/issues",
    "wiki": "https://github.com/digitale-helden/wp-logger/wiki",
    "source": "https://github.com/digitale-helden/wp-logger"
  },
  "config": {
    "vendor-dir": "lib/vendor"
  },
  "require": {
    "php": ">=7.4.1",
    "graylog2/gelf-php": "^1.7",
    "monolog/monolog": "^2.3"
  },
  "autoload": {
    "psr-4": {
      "DigitaleHelden\\Logger\\": "src/"
    }
  }
}
